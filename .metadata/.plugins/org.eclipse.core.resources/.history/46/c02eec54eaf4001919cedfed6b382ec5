package patterns;

public class Pyramid4 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int i,j;
		for( i=1;i<=5;i++)
		{
			//for( j=5;j>i;j--) 
			{
				System.out.print(" ");
		}
			//for( j=1;j<=i;j++)
			{
				System.out.print(j);
			}
			//for( j=j-2;j>=1;j--)
			{
				System.out.print(j);
			}
			System.out.println();
	}

}
}
